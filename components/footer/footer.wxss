.footer {
	position: fixed;
	bottom: 0;
	left: 0;
	z-index: 11000;
	display: flex;
	justify-content: space-around;
	align-items: center;
	text-align: center;
	width: 100%;
	height: 100rpx;
	border-top: 1px solid #F3F3F3;
	font-size: 23rpx;
	color: #C9C9C9;
	background: white;
	
}

.selected {
	color: #FF6D23;
}

.footer view:nth-child(1) image {
	margin-bottom: 8rpx;
	width: 42rpx;
	height: 51rpx;
}

.footer view:nth-child(2) image {
	width: 34rpx;
	height: 49rpx; 
}


.footer view:nth-child(3) image {
	width: 37rpx;
	height: 49rpx;
}
