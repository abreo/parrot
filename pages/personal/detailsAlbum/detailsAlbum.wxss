.contain {
	position: fixed;
	left: 0;
	top: 0;
	right: 0;
	bottom: 0;
	overflow-y: auto;
	background: #efeff4;
}
.contain .bg {
	width: 100%;
	height: 348rpx;
}

.section {
	position: absolute;
	left: 0;
	top: 255rpx;
	width: 100%;
	background: #fff;
	border-radius: 20rpx 20rpx 0 0;
}
.section .info{
	margin: 0 33rpx 0rpx 37rpx;
	padding-bottom: 60rpx;
}
.section .info .head {
	position: relative;
	height: 175rpx;
	border-bottom: 1rpx solid #D1D1D1;
}
.section .info .head .details {
	position: absolute;
	left: 26rpx;
	top: -41rpx;
}
.section .info .head .details .photo {
	width: 115rpx;
	height: 115rpx;
	border-radius: 50%;
}
.section .info .head .details .user {
	margin-top: 11rpx;
	font-size: 25rpx;
	color: #4E4E4E;
}
.section .info .head .details .count {
	margin-top: 6rpx;
	font-size: 20rpx;
	color: #8E8D8D;
}
.section .info .head button {
	float: right;
	margin-right: 20rpx;
	margin-top: 42rpx;
	width: 196rpx;
	height: 90rpx;
	font-size: 32rpx;
	color: #fff;
	letter-spacing: 5rpx;
	background: #d2d2d2;
	border-radius: 90rpx;
}

.section .info .main .nav {
	display: flex;
	justify-content: space-around;
}
.section .info .main .nav view{
	margin-top: 18rpx;
}
.section .info .main .describe {
	display: block;
}

