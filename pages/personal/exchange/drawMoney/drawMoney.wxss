.classify {
	margin-top: 66rpx;
	display: flex;
	flex-direction: column;
	align-items: center;
	font-size: 30rpx;
	color: #4E4E4E;
}
.classify .surplus {
	margin-top: 25rpx;
	display: flex;
	align-items: center;
	width: 443rpx;
	height: 70rpx;
	border: 1rpx solid #C9C9C9;
	border-radius: 10rpx;
}
.classify .surplus view:nth-child(1) {
	padding-left: 27rpx;
	width: 275rpx;
	color: #C1C1C6;
}
.classify .surplus view:nth-child(2) {
	flex: 1;
}
